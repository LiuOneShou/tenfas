/* componet/List/List.wxss */

.contentbox {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}

.content {
	width: 49%;
	border-radius: 15rpx;
	overflow: hidden;
	background-color: rgb(246, 247, 246);
	margin-bottom: 10rpx;
}

.content image {
	width: 100%;
	height: 250rpx;
}

.carname {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-weight: 600;
}

.deploybox {
	padding: 15rpx;
}

.deploy {
	margin: 10rpx 0;
	font-size: 24rpx;
	color: rgb(182, 187, 182);
	display: flex;
}

.gang {
	margin: 0 5rpx;
}

.price {
	color: red;
	font-weight: 600;
}